{
    "name": "chrisdbrown/yaml-standardiser",
    "description": "Yaml Standardiser",
    "license": [
        "GPL-3.0-only"
    ],
    "require": {
        "php": "^7.1, <7.3",
        "symfony/console": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "consistence/coding-standard": "~2.0",
        "phpstan/phpstan": "^0.11.4"
    },
    "autoload": {
        "psr-4": {
            "YamlStandardiser\\": [
                "src/"
            ]
        }
    },
    "bin": [
        "bin/yaml-standardiser"
    ],
    "config": {
        "sort-packages": true
    }
}
